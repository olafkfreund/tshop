{
  "name": "tshop",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered custom apparel platform",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest --watch --passWithNoTests",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate || echo 'Skipping prisma generate'",
    "analyze": "ANALYZE=true npm run build",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags=\"--headless\"",
    "perf:check": "npm run build && npm run perf:lighthouse"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@google/generative-ai": "^0.19.0",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.15.0",
    "@sentry/nextjs": "^10.9.0",
    "@sentry/profiling-node": "^10.9.0",
    "@stripe/stripe-js": "^4.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.5",
    "@types/three": "^0.179.0",
    "@vercel/analytics": "^1.0.0",
    "@vercel/speed-insights": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.0",
    "fabric": "^6.7.1",
    "lucide-react": "^0.400.0",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.29",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prisma": "^5.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^16.12.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.17",
    "three": "^0.167.1",
    "typescript": "^5.6.0",
    "web-push": "^3.6.7",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/fabric": "^5.3.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "tsx": "^4.0.0"
  }
}
