{
  "name": "tshop",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered custom apparel platform with comprehensive NixOS development environment",
  "keywords": [
    "nextjs",
    "ai",
    "ecommerce",
    "custom-apparel",
    "fabric.js",
    "three.js",
    "stripe",
    "prisma",
    "nixos"
  ],
  "author": "TShop Development Team",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "next dev --turbo --port 3000",
    "build": "next build",
    "start": "next start --port 3000",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true next build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio --port 5555",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.0",
    "@google/generative-ai": "^0.16.0",
    "@hookform/resolvers": "^3.7.0",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/xr": "^6.2.0",
    "@reduxjs/toolkit": "^2.2.0",
    "@stripe/stripe-js": "^4.1.0",
    "@vercel/analytics": "^1.3.0",
    "@vercel/speed-insights": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "cloudinary": "^2.4.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "fabric": "^6.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.400.0",
    "next": "^15.0.0",
    "next-auth": "5.0.0-beta.20",
    "next-i18next": "^15.3.0",
    "postcss": "^8.4.0",
    "prisma": "^5.17.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.52.0",
    "react-i18next": "^15.0.0",
    "react-redux": "^9.1.0",
    "sharp": "^0.33.0",
    "stripe": "^16.8.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^4.0.0",
    "three": "^0.167.0",
    "typescript": "^5.6.0",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@storybook/react-vite": "^8.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/fabric": "^5.3.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.167.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "chromatic": "^11.5.0",
    "class-variance-authority": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tsx": "^4.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/tshop.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/tshop/issues"
  },
  "homepage": "https://github.com/your-org/tshop#readme"
}